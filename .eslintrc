{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "prettier"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "linebreak-style": [
      "off"
    ],
    "react/jsx-filename-extension": [
      "off",
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "arrow-parens": "off",
    "comma-dangle": "off",
    "quotes": [
      "error",
      "single",
      {
        "allowTemplateLiterals": true
      }
    ]
  }
}
