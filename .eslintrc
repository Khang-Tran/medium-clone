{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "linebreak-style": [
      "warn"
    ],
    "react/jsx-filename-extension": [
      "off",
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "arrow-parens": "off",
    "comma-dangle": "off"
  }
}
